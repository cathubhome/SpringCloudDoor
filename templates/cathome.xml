<templateSet group="cathome">
  <template name="amd" value="/**&#10; *author: $author$&#10; *@time: $date$ $time$&#10; *description： $description$&#10; *@method: $method$&#10; *params： $param$&#10; *return： $methodreturntype$&#10; */" description="Add method definition(添加方法注释)" toReformat="false" toShortenFQNames="true">
    <variable name="author" expression="user()" defaultValue="wangyq" alwaysStopAt="false" />
    <variable name="date" expression="date()" defaultValue="" alwaysStopAt="true" />
    <variable name="time" expression="time()" defaultValue="" alwaysStopAt="false" />
    <variable name="description" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="method" expression="methodName()" defaultValue="" alwaysStopAt="true" />
    <variable name="param" expression="methodParameters()" defaultValue="" alwaysStopAt="true" />
    <variable name="methodreturntype" expression="methodReturnType()" defaultValue="" alwaysStopAt="false" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name="acd" value="/**&#10; *&lt;pre&gt;&#10; *  Created with IntelliJ IDEA&#10; *  Author: $author$&#10; *  Email: 1015726552@qq.com&#10; *  Time: $time$&#10; *  Class: $class_name$&#10; *  Package: $package_name$&#10; *  Description: $desc$&#10; *  Version: 1.0&#10; *&lt;/pre&gt;&#10; */" description="Add java class definition(添加类注释)" toReformat="false" toShortenFQNames="true">
    <variable name="author" expression="user()" defaultValue="" alwaysStopAt="false" />
    <variable name="time" expression="time(&quot;yyyy-MM-dd HH:mm:ss&quot;)" defaultValue="" alwaysStopAt="false" />
    <variable name="class_name" expression="className()" defaultValue="" alwaysStopAt="false" />
    <variable name="package_name" expression="currentPackage()" defaultValue="" alwaysStopAt="false" />
    <variable name="desc" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
</templateSet>